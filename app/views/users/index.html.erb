<% provide(:title, "List all users")  %>
<h1 class="header-title">Listing users</h1>

<%= will_paginate %>

<ul class="users">
    <%= @users.each do |user| %>
        <li>
            <%= gravatar_for user %>
            <%= link_to user.name, user_path(user) %>
        </li>
    <% end %>
</ul>

<%= will_paginate %>
